@model IEnumerable<appProyectoDSWI_Tienda.Models.Fabricante>

@{
    ViewBag.Title = "listadoFabricantes";
    Layout = "~/Views/Shared/_LayoutPrincipal.cshtml";
}

<h2>LISTADO DE FABRICANTES</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idFabr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nomFabr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.paisFabr)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idFabr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nomFabr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.paisFabr)
        </td>
    </tr>
}

</table>
